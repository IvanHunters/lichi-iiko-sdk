{
  "name": "lichi/iiko-sdk",
  "type": "library",
  "description": "PHP SDK for IIKO (Card/Biz/Cloud/Transport)",
  "keywords": ["Lichi","lichi", "iiko php sdk", "iiko", "iiko api", "iiko card", "iiko transport", "iiko biz", "iiko cloud"],
  "homepage": "https://github.com/IvanHunters/lichi-iiko-sdk",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Okhotnikov",
      "email": "ohotnikovivan@yandex.ru"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "guzzlehttp/guzzle": "~6",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "autoload": {
    "psr-4": {
      "Lichi\\Iiko\\": "src/",
      "Lichi\\Iiko\\Sdk\\": "src/sdk/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
